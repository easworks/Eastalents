{
  "name": "accounts-client-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/accounts-client-e2e/src",
  "tags": [],
  "implicitDependencies": ["accounts-client"],
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/accounts-client-e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "accounts-client:serve:development"
      },
      "configurations": {
        "production": {
          "devServerTarget": "accounts-client:serve:production"
        },
        "ci": {
          "devServerTarget": "accounts-client:serve-static"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
